public DownloadInfo getDownloadInfo(String title){

  for(int i = 0; i < downloadList.size(); i++){
  
    if(downloadList.get(i).getTitle().equals(title)){
    
   return downloadList.get(i);
    
    }
      

  }
  
  return null;
}
2013.3.b
public void updateDownloads(List<String> titles ){
  for(int i = 0; i < titles.size(); i ++){
    String a = titles.get(i);
   DownloadInfo compare = getDownloadInfo(a);
    if(compare != null){
    compare.incrementTimesDownloaded();
    
    }else{
      DownloadInfo add = new DownloadInfo(a);
      downloadList.add(add);
    
    
    }
      
  
  }
  
}
