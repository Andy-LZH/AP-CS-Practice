public static String scrambleWord(String Word){
  String use = Word.substring(0,1);
  String persv = "";
  for(int i = 1; i < Word.length(); i ++){
      if(use = "A"){
        String swap = Word.substring(i,i+1);
        persv += swap;
        persv += use; 
	i++;
     }else{
     use = Word.substring(i,i+1);
     persv += use;
     }
  }
return persv;
}
